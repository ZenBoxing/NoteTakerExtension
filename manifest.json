{
    "name": "Note Taker App",
    "version": "1.0",
    "manifest_version": 3,
    "action":{
      "default_popup":"popup.html",
      "default_title":"Open the popup"
    },
    "background":{
        "service_worker": "background.js"
    },
    "icons": 
      {
        "128": "NoteTakerIcon1.png",
        "48": "NoteTakerIcon48.png",
        "16": "NoteTakerIcon16.png"
      }
    ,
    "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "contextMenus"
    ],
    "host_permissions":[
      "*://*/*"
    ]
  
  }
  