{
    "name": "Note Taker App",
    "version": "1.0",
    "manifest_version": 3,
    "action":{
      "default_popup":"popup.html",
      "default_title":"Note Taker"
    },
    "background":{
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": 
      {
        "128": "Note-Taker-Icon128.png",
        "48": "Note-Taker-Icon48.png",
        "16": "Note-Taker-Icon16.png"
      }
    ,
    "key": "",
    "oauth2": {
      "client_id": "930822050765-ui2uve3suebm5n2dc1ipp4j1ngj0n82i.apps.googleusercontent.com",
      "scopes":[
        "profile email",
        "https://www.googleapis.com/auth/documents"
      ]
    }, 
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "contextMenus",
      "identity",
      "identity.email"
    ],
    "host_permissions":[
      "*://*/*"
    ]
  
  }
  